dd =[]

#functions
    
def yylist():
    n = 1
    while n < len(tt) :
        y = eval( '-4.901*tt[n]**2 + 20')
        yy.append(y)
        n += 1
        
def difference() :
    n = 0
    while n < len(yy) :
        d = yy[n] - yyn[n]
        dd.append(-d)
        n += 1

def solver(left, right, var): #
    eq = str(left) + '-' + str(right)
    a = sp.solve(eq, var)
    print('Integrating then solving for %s :' % (str(var)))
    print('%s = ' % (str(var)),str(a[0]) + '+' + str(c)) 
    return(str(a[0]) + '+' + str(c))


#body 
    #integration
y = Symbol('y') 
v = Symbol('v')
t = Symbol('t')
c = Symbol('c') #integration constant
leftI = integrate(1,v) # I stands for integration
rightI = integrate(a,t)
leftIsec = integrate(1,y) #sec stands for second integration
rightIsec = integrate(rightI+v0,t)
VEq = solver(leftI, rightI, v) #solves for the variable v
eq = 'v - (' + str(VEq) + ")"
v = v0
t = t0
VCVal = sp.solve(eval(eq), c)[0]
print("constant : " ,VCVal)
VelEq = VEq.replace('c', str(VCVal))
print('Final eq for v:\n v = ' , VelEq, '\n\n')

YEq = solver(leftIsec, rightIsec, y) 
eq = 'y - (' + str(YEq) + ")"
y = y0
t = t0
YCVal = sp.solve(eval(eq), c)[0]
print("constant : " ,YCVal)
PosEq = YEq.replace('c', str(YCVal))
print('Final eq for y:\n y = ' , PosEq, '\n\n')



#plotting

yylist()
t = np.asarray(tt)
y = -4.901*t**2 + 20
plt.figure()
plt.plot(t , y)
plt.xlabel('time')
plt.ylabel('position')
difference()

#errors
plt.figure()
plt.plot(tt,dd)
plt.xlabel('time')
plt.ylabel('difference')
