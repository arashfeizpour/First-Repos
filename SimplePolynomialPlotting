import numpy as np  
import matplotlib.pyplot as plt 
 
def graph(formula, x_range):  
    x = np.array(x_range)  
    y = eval(formula)
    plt.plot(y,x)
    plt.xlabel('x')
    plt.ylabel('y')    
    plt.show()
    
graph('x**3+2*x-4', range(-10, 11))
