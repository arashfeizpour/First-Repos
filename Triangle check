print('a ra vared knid : /n')
a = int(input())
print('b ra vared knid : /n')
b = int(input())
print('c ra vared knid : /n')
c = int(input())
if(a + b > c and a + c > b and c + b > a):
    print ("Triangle")
else :
    print("Not a Triangle")
        
